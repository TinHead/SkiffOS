# Docker Engine and dependencies
BR2_PACKAGE_DOCKER_ENGINE=y
BR2_PACKAGE_DOCKER_ENGINE_EXPERIMENTAL=y
BR2_PACKAGE_DOCKER_CLI=y
BR2_PACKAGE_DOCKER_CLI_BUILDX=y
# BR2_PACKAGE_DOCKER_CLI_STATIC is not set
# BR2_PACKAGE_DOCKER_ENGINE_DRIVER_AUFS is not set
# BR2_PACKAGE_DOCKER_ENGINE_DRIVER_DEVICEMAPPER is not set
BR2_PACKAGE_LIBFUSE=y

# Add the tini init binary.
BR2_PACKAGE_TINI=y

# Add the docker compose CLI plugin.
BR2_PACKAGE_DOCKER_COMPOSE=y
